<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.papyrus.gendoc2.serviceTypes">
      <serviceType
            id="HtmlService"
            interface="org.eclipse.papyrus.gendoc2.tags.html.IHtmlService">
      </serviceType>
   </extension>
   <extension
         point="org.eclipse.papyrus.gendoc2.services">
      <service
            class="org.eclipse.papyrus.gendoc2.tags.html.impl.HtmlServiceFactory"
            default="true"
            id="HtmlService"
            serviceType="HtmlService">
      </service>
   </extension>
   <extension
         point="org.eclipse.papyrus.gendoc2.tags">
      <category
            name="org.eclipse.papyrus.gendoc2.tags.scripts">
         <tag
               handler="org.eclipse.papyrus.gendoc2.tags.html.impl.RichTextTagHandler"
               name="richText">
            <attribute
                  default="default"
                  name="version"
                  required="false"
                  type="java.lang.String">
            </attribute>
         </tag>
      </category>
   </extension>
   <extension
         point="org.eclipse.papyrus.gendoc2.processes">
      <process
            id="additionalStyles"
            label="Adding additional styles"
            parallel="false"
            priority="NORMAL"
            processor="org.eclipse.papyrus.gendoc2.tags.html.AdditionalStylesProcess">
         <predecessor
               ref="generate">
         </predecessor>
         <successor
               ref="save">
         </successor>
      </process>
   </extension>

</plugin>
