<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.eclipse.papyrus.gendoc2.wizard.runner" name="%extension-point.name" schema="schema/org.eclipse.papyrus.gendoc2.wizard.runner.exsd"/>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.eclipse.papyrus.gendoc2.wizard.popup.actions.GenerateDocumentationHandler"
            description="%command.description"
            id="org.eclipse.papyrus.gendoc2.wizard.GenerateDoc"
            name="%command.name">
      </command>
      <command
            defaultHandler="org.eclipse.papyrus.gendoc2.wizard.popup.actions.GenerateDocumentationModelExplorerHandler"
            description="%command.description"
            id="org.eclipse.papyrus.gendoc2.wizard.GenerateDocModelExplorer"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any">
         <command
               commandId="org.eclipse.papyrus.gendoc2.wizard.GenerateDoc"
               icon="icons/GenerateDoc.gif"
               label="%command.label"
               style="push"
               tooltip="%command.tooltip">
             <visibleWhen checkEnabled="false">
                <iterate ifEmpty="false">
                   <test property="org.eclipse.papyrus.gendoc2.wizard.isEnabled" forcePluginActivation="true"/>
                </iterate>
             </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="org.eclipse.papyrus.gendoc2.wizard.WizardPropertyTester"
            id="org.eclipse.papyrus.gendoc2.wizard.propertytester"
            namespace="org.eclipse.papyrus.gendoc2.wizard"
            properties="isEnabled"
            type="java.lang.Object">
      </propertyTester>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            locationURI="popup:org.eclipse.papyrus.views.modelexplorer.modelexplorer.popup">
         <command
               commandId="org.eclipse.papyrus.gendoc2.wizard.GenerateDocModelExplorer"
               icon="icons/GenerateDoc.gif"
               label="%command.label"
               style="push"
               tooltip="%command.tooltip">
         </command>
      </menuContribution>
   </extension>

</plugin>
