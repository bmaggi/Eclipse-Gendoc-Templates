<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.eclipse.papyrus.gendoc2.documentTypes" name="%extension-point.name" schema="schema/documentTypes.exsd"/>
   <extension
         point="org.eclipse.papyrus.gendoc2.serviceTypes">
      <serviceType
            id="DocumentManager"
            interface="org.eclipse.papyrus.gendoc2.documents.IDocumentManager">
      </serviceType>
      <serviceType
            id="DocumentService"
            interface="org.eclipse.papyrus.gendoc2.documents.IDocumentService">
      </serviceType>
   </extension>
   <extension
         point="org.eclipse.papyrus.gendoc2.services">
      <service
            class="org.eclipse.papyrus.gendoc2.documents.impl.DocumentManager"
            default="true"
            id="DocumentManager"
            serviceType="DocumentManager">
      </service>
      <service
            class="org.eclipse.papyrus.gendoc2.documents.impl.DocumentServiceFactory"
            default="true"
            id="DocumentService"
            serviceType="DocumentService">
      </service>
   </extension>

</plugin>
